{
  "name": "generator-landingpages",
  "version": "0.1.5",
  "description": "Yeoman generator for simple landing page",
  "homepage": "",
  "author": {
    "name": "Guillermo de la Iglesia",
    "email": "mail@zguillez.io",
    "url": "https://github.com/zguillez"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "web",
    "landing",
    "mustache",
    "sass",
    "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "nsp": "^3.1.0",
    "eslint": "^5.0.0",
    "prettier": "^1.10.2",
    "husky": "^0.14.3",
    "lint-staged": "^6.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-prettier": "^2.5.0",
    "eslint-config-xo": "^0.19.0",
    "jest": "^22.1.4"
  },
  "dependencies": {
    "yeoman-generator": "^2.0.2",
    "chalk": "^2.3.0",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublish": "nsp check",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": "https://github.com/zguillez/generator-landingpages",
  "license": "MIT"
}
